{
  "name": "sftp-cache",
  "version": "0.0.13",
  "description": "Cache via sftp. Supports caching files and redownloading them",
  "main": "src/index.js",
  "engines": {
    "node": ">=10.14.2"
  },
  "scripts": {
    "format": "prettier --write '{src/**/*.js,*.{js,json}}'",
    "lint": "eslint src",
    "test": "jest --collect-coverage"
  },
  "keywords": [
    "node",
    "cache",
    "ssh",
    "ssh2",
    "md5",
    "hash",
    "sftp",
    "compare",
    "assets",
    "upload",
    "download"
  ],
  "author": "Benedikt RÃ¶tsch <opensource@axe312.de>",
  "bugs": {
    "url": "https://github.com/axe312ger/sftp-cache/issues"
  },
  "homepage": "https://github.com/axe312ger/sftp-cache/",
  "repository": {
    "type": "git",
    "url": "https://github.com/axe312ger/sftp-cache.git"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write --config .prettierrc",
      "npm run test",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write --config .prettierrc"
    ]
  },
  "files": [
    "src/index.js",
    "src/filesystem.js",
    "src/sftp.js",
    "src/diff.js"
  ],
  "dependencies": {
    "execa": "^5.0.0",
    "fast-glob": "^3.2.5",
